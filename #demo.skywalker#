routine Main
  a = 0;
  b = 0;
  c = 0;

  wait(2);

  a = 42;
  b = 37;
  c = 59;

  wait(2);

  a = 10;
  b = 15;
  c = 20;

  wait(2);

  a = 30;
  b = 40;
  c = 50;

  wait(3);

  a = 0;
  b = 0;
  c = 0
end


rule :if_Stmt do

end

rule else_stmt do

end

rule if_else_stmt
   match(:if_stmt)
   match(if_stmt, else_stmt)